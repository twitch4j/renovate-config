{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "regexManagers": [
    {
      "fileMatch": [
        ".*\\.kts$"
      ],
      "datasourceTemplate": "maven",
      "matchStrings": [
        "// renovate: depName=(?<depName>.*?)\r?\n.*prefer(\"(?<currentValue>.*)\")"
      ]
    }
  ]
}
